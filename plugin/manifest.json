{
  "name": "Relevance",
  "version": "0.0",
  "manifest_version": 2,
  "description": "Discover articles that impact your friends.",
  "permissions": ["tabs", "activeTab", "webNavigation", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
